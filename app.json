{
  "expo": {
    "name": "90skalyanam",
    "slug": "90skalyanam",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "90skalyanam",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "androidStatusBar": {
      "visible": true
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#E02A30"
      },
      "edgeToEdgeEnabled": true,
      "fullscreen": false,
      "theme": "@android:style/Theme.DeviceDefault.NoActionBar"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "android": {
            "statusBar": {
              "hidden": false
            }
          }
        }
      ],
      "expo-video",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
